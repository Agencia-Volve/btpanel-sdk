{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "quotes": [2, "single", { "avoidEscape": true }],
      "import/extensions": "off",
      "import/no-unresolved": "off",
      "camelcase": "off",
      "import/first": "off",
      "import/prefer-default-export": "off",
      "dot-notation": "off"
    }
}
